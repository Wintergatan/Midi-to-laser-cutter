{
  "name": "musicboxeditor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "typings install",
    "build": "webpack --progress",
    "build:dev": "webpack --progress --watch",
    "build:prod": "rimraf build && cross-env APP_ENVIRONMENT=production webpack -p --progress",
    "server": "lite-server -c lite-server.config.js",
    "server:gzip": "cross-env APP_ENVIRONMENT=gzip lite-server -c lite-server.config.js",
    "test:dev": "cross-env APP_ENVIRONMENT=test TEST_ENVIRONMENT=dev ./node_modules/karma/bin/karma start ./karma.config.js",
    "test:prod": "cross-env APP_ENVIRONMENT=test ./node_modules/karma/bin/karma start ./karma.config.js"
  },
  "dependencies": {
    "@angular/common": "2.2.3",
    "@angular/compiler": "2.2.3",
    "@angular/compiler-cli": "2.2.3",
    "@angular/core": "2.2.3",
    "@angular/forms": "2.2.3",
    "@angular/http": "2.2.3",
    "@angular/platform-browser": "2.2.3",
    "@angular/platform-browser-dynamic": "2.2.3",
    "@angular/platform-server": "2.2.3",
    "@angular/router": "3.2.3",
    "core-js": "2.4.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.26",
	"midiconvert": "^0.4.1"
  },
  "devDependencies": {
    "@ngtools/webpack": "1.1.9",
    "@types/angular": "^1.6.10",
    "@types/node": "^7.0.12",
    "@types/webpack-env": "^1.13.0",
    "angular2-template-loader": "0.6.0",
    "compression": "^1.6.2",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "3.1.3",
    "enhanced-resolve": "3.0.0",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "2.24.1",
    "http-proxy-middleware": "^0.17.4",
    "jasmine-core": "^2.5.2",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.30",
    "karma-webpack": "^2.0.3",
    "lite-server": "^2.3.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "ts-loader": "1.3.0",
    "typescript": "2.0.10",
    "typings": "2.0.0",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-server": "2.1.0-beta.12",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
